    "test:integration": "bun test __tests__/integration/",
    "test:e2e": "bun test __tests__/e2e/",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "elizaos",
    "plugin",
    "n8n",
    "workflow",
    "automation",
    "ai",
    "natural-language"
